{
    "version": 2,
    "builds": [
      {
        "src": "api/timesheet.py",
        "use": "@vercel/python"
      }
    ],
    "routes": [
      {
        "src": "/api/(.*)",
        "dest": "/api/timesheet.py"
      }
    ]
  }
  {
    "routes": [
      { "src": "/manifest.json", "dest": "/public/manifest.json" }
    ]
  }
  